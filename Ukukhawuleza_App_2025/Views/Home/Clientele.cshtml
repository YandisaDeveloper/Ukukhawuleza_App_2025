@{
    ViewData["Title"] = "Our Clientele";
}

<section class="clientele-section py-5">
    <div class="container">
        <h2 class="section-title text-center mb-5">Our Esteemed Clients</h2>
        <div class="logo-grid">
            <div class="logo-item" title="Client 1" data-aos="fade-up">
                <img src="/CC logo.png" alt="Client 1 Logo" />
            </div>
            <div class="logo-item" title="Client 2" data-aos="fade-up" data-aos-delay="100">
                <img src="/CEMI LOGO.png" alt="Client 2 Logo" />
            </div>
            <div class="logo-item" title="Client 3" data-aos="fade-up" data-aos-delay="200">
                <img src="/prasa-logo.png" alt="Client 3 Logo" />
            </div>
            <div class="logo-item" title="Client 4" data-aos="fade-up" data-aos-delay="300">
                <img src="/Randwaterlogo.png" alt="Client 4 Logo" />
            </div>
            <div class="logo-item" title="Client 5" data-aos="fade-up" data-aos-delay="400">
                <img src="/Transnet Logo.png" alt="Client 5 Logo" />
            </div>
            <div class="logo-item" title="Client 6" data-aos="fade-up" data-aos-delay="500">
                <img src="/kig logo.png" alt="Client 6 Logo" />
            </div>
            <div class="logo-item" title="Client 6" data-aos="fade-up" data-aos-delay="500">
                <img src="/Umzimvubu-Logo.png" alt="Client 6 Logo" />
            </div>
            <!-- Add more logos as needed -->
            <div class="logo-item no-logo d-flex align-items-center justify-content-center" title="Client 5" data-aos="fade-up" data-aos-delay="400" style="min-width: 150px; min-height: 80px; background-color: #f0f0f0; border-radius: 8px; font-weight: 600; font-size: 1.1rem; color: #333;">
                <span>Maziya General Sercives</span>
            </div>
        </div>
    </div>
</section>

<link href="https://fonts.googleapis.com/css2?family=Inter:wght@400;600&family=Merriweather:wght@700&display=swap" rel="stylesheet" />
<link href="https://unpkg.com/aos@2.3.1/dist/aos.css" rel="stylesheet" />



<script src="https://unpkg.com/aos@2.3.1/dist/aos.js"></script>
<script>
    AOS.init({
      duration: 900,
      once: true,
    });
</script>
